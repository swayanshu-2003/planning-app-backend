{
    "version": 2,
    "builds": [
        {
            "src": "src/index.ts",
            "use": "@vercel/node"
        },
        {
            "src": "prisma",
            "use": "@prisma/cli"
        }
    ],
    "routes": [
        {
            "src": "/api/v1/(.*)",
            "dest": "src/index.ts"
        }
    ]
}